<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:networkSecurityConfig="@xml/network_security_config"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/gantengin"
        android:label="@string/app_name"
        android:roundIcon="@drawable/gantengin"
        android:supportsRtl="true"
        android:theme="@style/Theme.LearnAndroidDasar">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="YOUR_ACTUAL_API_KEY_HERE" />

        <activity
            android:name="com.gantenginapp.apps.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.LearnAndroidDasar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.gantenginapp.apps.ui.screen.Splash.SplashActivity"
            android:exported="true"
            android:label="Intro" />

        <!-- LoginActivity -->
        <activity
            android:name="com.gantenginapp.apps.ui.screen.onBoarding.LandingActivity"
            android:exported="true"
            android:label="Login" />

        <activity
            android:name="com.gantenginapp.apps.ui.screen.login.LoginActivity"
            android:exported="true"
            android:label="Login" />

        <activity
            android:name="com.gantenginapp.apps.ui.screen.register.RegisterActivity"
            android:exported="true"
            android:label="Login" />

        <activity
            android:name="com.gantenginapp.apps.ui.screen.home.HomeActivity"
            android:exported="true"
            android:label="Login" />

        <activity
            android:name="com.gantenginapp.apps.ui.screen.profil.ProfileActivity"
            android:exported="true"
            android:label="Login" />

        <activity
            android:name="com.gantenginapp.apps.ui.screen.registstore.RegistStoreActivity"
            android:exported="true"
            android:label="Login" />

        <activity
            android:name="com.gantenginapp.apps.ui.screen.StoreBarber.BarberStoreActivity"
            android:exported="true"
            android:label="Login" />

        <activity
            android:name="com.gantenginapp.apps.ui.screen.adminstore.AdminStoreActivity"
            android:exported="true"
            android:label="Login" />



    </application>

</manifest>